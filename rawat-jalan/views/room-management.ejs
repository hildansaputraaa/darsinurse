<!DOCTYPE html>
<html lang="id">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Manajemen Ruangan - Darsinurse</title>
  <link rel="icon" type="image/png" href="data:image/svg+xml,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 100 100'><text y='0.9em' font-size='90'>üè•</text></svg>">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
  
  <style>
    :root {
      --primary-green: #00B14F;
      --secondary-teal: #20c997;
      --primary-blue: #0056b3;
      --danger-red: #dc3545;
    }

    * { margin: 0; padding: 0; box-sizing: border-box; }

    body {
      font-family: 'Inter', 'Segoe UI', -apple-system, BlinkMacSystemFont, sans-serif;
      background: linear-gradient(135deg, #f5f7fa 0%, #c3cfe2 100%);
      min-height: 100vh;
    }

    .top-header {
      background: rgba(255, 255, 255, 0.95);
      padding: 1.2rem 0;
      box-shadow: 0 4px 20px rgba(0,0,0,0.08);
      position: sticky;
      top: 0;
      z-index: 1000;
    }

    .logo-info {
      display: flex;
      align-items: center;
      gap: 14px;
    }

    .logo-info i {
      font-size: 36px;
      color: var(--primary-green);
    }

    .logo-info h1 {
      font-size: 26px;
      font-weight: 800;
      color: #1a3a3a;
      margin: 0;
    }

    .logo-info small {
      display: block;
      font-size: 12px;
      color: #6c757d;
      margin-top: 2px;
    }

    .user-info {
      display: flex;
      align-items: center;
      gap: 18px;
      font-size: 14px;
    }

    .user-info a {
      color: var(--primary-blue);
      text-decoration: none;
      font-weight: 600;
      padding: 6px 12px;
      border-radius: 8px;
    }

    .user-info a:hover {
      color: var(--primary-green);
      background: rgba(0,177,79,0.1);
    }

    .main-container {
      padding: 28px 0;
    }

    .page-title {
      background: rgba(255, 255, 255, 0.95);
      padding: 22px 26px;
      border-radius: 16px;
      box-shadow: 0 4px 16px rgba(0,0,0,0.12);
      margin-bottom: 28px;
    }

    .page-title h2 {
      margin: 0;
      color: #1a3a3a;
      display: flex;
      align-items: center;
      gap: 14px;
      font-weight: 800;
      font-size: 24px;
    }

    .page-title h2 i {
      color: var(--primary-green);
      font-size: 32px;
    }

    .search-container {
      display: flex;
      align-items: center;
      gap: 12px;
      margin-top: 15px;
    }

    .search-input {
      flex: 1;
      padding: 10px 15px;
      border: 2px solid #e0e0e0;
      border-radius: 25px;
      font-size: 14px;
      transition: all 0.3s;
    }

    .search-input:focus {
      border-color: var(--primary-green);
      box-shadow: 0 0 0 0.2rem rgba(0,177,79,0.15);
      outline: none;
    }

    .btn-search {
      background: var(--primary-green);
      border: none;
      padding: 10px 20px;
      border-radius: 25px;
      color: white;
      font-weight: 600;
      cursor: pointer;
      transition: all 0.3s;
    }

    .btn-search:hover {
      background: #00a143;
      transform: scale(1.05);
    }

    .btn-add-room {
      background: var(--primary-green);
      border: none;
      padding: 11px 24px;
      border-radius: 11px;
      color: white;
      font-weight: 700;
      font-size: 14px;
      cursor: pointer;
      transition: all 0.3s;
      box-shadow: 0 4px 16px rgba(0,177,79,0.25);
    }

    .btn-add-room:hover {
      transform: translateY(-3px);
      box-shadow: 0 8px 24px rgba(0,177,79,0.35);
      background: #00a143;
    }

    .room-card {
      background: white;
      border-radius: 16px;
      box-shadow: 0 2px 15px rgba(0,0,0,0.08);
      overflow: hidden;
      margin-bottom: 20px;
      transition: all 0.3s;
      animation: fadeInUp 0.6s ease-out forwards;
      opacity: 0;
      transform: translateY(20px);
    }

    .room-card:nth-child(1) { animation-delay: 0.1s; }
    .room-card:nth-child(2) { animation-delay: 0.2s; }
    .room-card:nth-child(3) { animation-delay: 0.3s; }
    .room-card:nth-child(4) { animation-delay: 0.4s; }
    .room-card:nth-child(5) { animation-delay: 0.5s; }
    .room-card:nth-child(6) { animation-delay: 0.6s; }

    @keyframes fadeInUp {
      to {
        opacity: 1;
        transform: translateY(0);
      }
    }

    .room-card:hover {
      transform: translateY(-8px);
      box-shadow: 0 12px 35px rgba(0,0,0,0.12);
    }

    .room-header {
      padding: 18px 20px;
      color: white;
      position: relative;
    }

    .room-header.occupied {
      background: linear-gradient(135deg, var(--primary-green) 0%, var(--secondary-teal) 100%);
    }

    .room-header.empty {
      background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%);
      color: #6c757d;
    }

    .room-status-badge {
      position: absolute;
      top: 15px;
      right: 15px;
      background: rgba(255,255,255,0.9);
      color: #495057;
      padding: 4px 12px;
      border-radius: 20px;
      font-size: 10px;
      font-weight: 700;
    }

    .room-id {
      font-size: 22px;
      font-weight: 800;
      margin: 0;
      display: flex;
      align-items: center;
      gap: 12px;
    }

    .room-body {
      padding: 18px 20px;
    }

    .info-row {
      display: flex;
      align-items: center;
      gap: 10px;
      margin-bottom: 8px;
      padding: 10px 12px;
      background: #f8f9fa;
      border-radius: 8px;
      border-left: 3px solid var(--primary-green);
      transition: all 0.3s;
    }

    .info-row:hover {
      background: #e9ecef;
      transform: translateX(5px);
    }

    .info-row i {
      color: var(--primary-green);
      width: 18px;
      text-align: center;
      font-size: 14px;
    }

    .info-label {
      font-weight: 600;
      color: #6c757d;
      min-width: 70px;
      font-size: 12px;
    }

    .info-value {
      color: #212529;
      font-weight: 600;
      font-size: 13px;
      flex: 1;
    }

    .room-actions {
      display: flex;
      gap: 6px;
      padding-top: 15px;
      margin-top: 12px;
      border-top: 2px solid #e9ecef;
    }

    .btn-room-action {
      flex: 1;
      padding: 8px 6px;
      border: none;
      border-radius: 8px;
      font-weight: 600;
      cursor: pointer;
      font-size: 12px;
      color: white;
      transition: all 0.3s;
    }

    .btn-assign { background: var(--primary-blue); }
    .btn-assign:hover { background: #003d7a; }

    .btn-remove { background: #fd7e14; }
    .btn-remove:hover { background: #e56910; }

    .btn-edit { background: #17a2b8; }
    .btn-edit:hover { background: #117a8b; }

    .btn-delete { background: var(--danger-red); }
    .btn-delete:hover { background: #c82333; }

    .modal-content {
      border-radius: 15px;
      border: none;
    }

    .modal-header {
      background: linear-gradient(135deg, var(--primary-green) 0%, var(--secondary-teal) 100%);
      color: white;
      border-radius: 15px 15px 0 0;
    }

    .form-label {
      font-weight: 600;
      color: #1a3a3a;
      margin-bottom: 8px;
    }

    .form-control, .form-select {
      border: 2px solid #e0e0e0;
      border-radius: 8px;
      padding: 10px 15px;
    }

    .form-control:focus, .form-select:focus {
      border-color: var(--primary-green);
      box-shadow: 0 0 0 0.2rem rgba(0,177,79,0.15);
    }

    .alert {
      border-radius: 10px;
      border: none;
      padding: 15px 20px;
      margin-bottom: 20px;
      animation: slideDown 0.3s ease;
    }

    @keyframes slideDown {
      from { opacity: 0; transform: translateY(-20px); }
      to { opacity: 1; transform: translateY(0); }
    }

    .empty-state {
      text-align: center;
      padding: 60px 20px;
      background: white;
      border-radius: 15px;
      box-shadow: 0 4px 15px rgba(0,0,0,0.1);
    }

    .empty-state i {
      font-size: 48px;
      color: #adb5bd;
      margin-bottom: 20px;
    }

    @media (max-width: 768px) {
      .page-title .d-flex { flex-direction: column; gap: 15px; }
      .btn-add-room { width: 100%; }
      .room-actions { flex-direction: column; }
      .btn-room-action { width: 100%; }
      .search-container { flex-direction: column; }
      .search-input { width: 100%; }
      .btn-search { width: 100%; }
    }
  </style>
</head>
<body>
  <header class="top-header">
    <div class="container-fluid">
      <div class="d-flex justify-content-between align-items-center">
        <div class="logo-info">
          <i class="fas fa-hospital"></i>
          <div>
            <h1>Darsinurse Gateway</h1>
            <small>Manajemen Ruangan</small>
          </div>
        </div>
        <div class="user-info">
          <span><i class="fas fa-user-nurse"></i> <%= nama_perawat %></span>
          <span>|</span>
          <a href="/dashboard"><i class="fas fa-home"></i> Dashboard</a>
          <span>|</span>
          <a href="/logout" onclick="return confirm('Yakin logout?')"><i class="fas fa-sign-out-alt"></i> Logout</a>
        </div>
      </div>
    </div>
  </header>

  <div class="container-fluid main-container">
    <div class="page-title">
      <div class="d-flex justify-content-between align-items-center flex-wrap">
        <h2><i class="fas fa-door-open"></i> Manajemen Ruangan</h2>
        <button class="btn btn-add-room" onclick="showAddRoomModal()">
          <i class="fas fa-plus"></i> Tambah Ruangan
        </button>
      </div>
      <div class="search-container">
        <input type="text" class="search-input" id="search-rooms" placeholder="Cari ruangan berdasarkan ID, device, atau pasien...">
        <button class="btn-search" onclick="filterRooms()">
          <i class="fas fa-search"></i> Cari
        </button>
      </div>
    </div>

    <div id="alert-container"></div>

    <div class="row" id="rooms-container">
      <div class="col-12 text-center">
        <div class="spinner-border text-success" role="status" style="width: 3rem; height: 3rem;"></div>
        <p class="mt-3 text-muted">Memuat data ruangan...</p>
      </div>
    </div>
  </div>

  <!-- Modal: Add Room -->
  <div class="modal fade" id="addRoomModal" tabindex="-1">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title"><i class="fas fa-plus-circle"></i> Tambah Ruangan Baru</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
        </div>
        <div class="modal-body">
          <form id="form-add-room">
            <div class="mb-3">
              <label class="form-label">Room ID <span class="text-danger">*</span></label>
              <input type="text" class="form-control" id="add-room-id" placeholder="Contoh: RUANG_001" required>
            </div>
            <div class="mb-3">
              <label class="form-label">Device ID <span class="text-danger">*</span></label>
              <input type="text" class="form-control" id="add-device-id" placeholder="Contoh: DEV_001" required>
            </div>
            <div class="mb-3">
              <label class="form-label">Pasien (Opsional)</label>
              <select class="form-select" id="add-emr-no">
                <option value="">-- Ruangan Kosong --</option>
              </select>
            </div>
            <div class="d-grid gap-2">
              <button type="submit" class="btn btn-success">
                <i class="fas fa-save"></i> Simpan Ruangan
              </button>
              <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">
                <i class="fas fa-times"></i> Batal
              </button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>

  <!-- Modal: Edit Room -->
  <div class="modal fade" id="editRoomModal" tabindex="-1">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title"><i class="fas fa-edit"></i> Edit Ruangan</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
        </div>
        <div class="modal-body">
          <form id="form-edit-room">
            <input type="hidden" id="edit-old-room-id">
            <div class="mb-3">
              <label class="form-label">Room ID <span class="text-danger">*</span></label>
              <input type="text" class="form-control" id="edit-room-id" required>
            </div>
            <div class="mb-3">
              <label class="form-label">Device ID <span class="text-danger">*</span></label>
              <input type="text" class="form-control" id="edit-device-id" required>
            </div>
            <div class="d-grid gap-2">
              <button type="submit" class="btn btn-primary">
                <i class="fas fa-save"></i> Update Ruangan
              </button>
              <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">
                <i class="fas fa-times"></i> Batal
              </button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>

  <!-- Modal: Assign Patient -->
  <div class="modal fade" id="assignPatientModal" tabindex="-1">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title"><i class="fas fa-user-plus"></i> Masukkan Pasien</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
        </div>
        <div class="modal-body">
          <div class="alert alert-info">
            <i class="fas fa-info-circle"></i>
            Ruangan: <strong id="assign-room-display"></strong>
          </div>
          <form id="form-assign-patient">
            <input type="hidden" id="assign-room-id">
            <div class="mb-3">
              <label class="form-label">Pilih Pasien <span class="text-danger">*</span></label>
              <select class="form-select" id="assign-emr-no" required>
                <option value="">-- Pilih Pasien --</option>
              </select>
            </div>
            <div class="d-grid gap-2">
              <button type="submit" class="btn btn-success">
                <i class="fas fa-check"></i> Masukkan Pasien
              </button>
              <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">
                <i class="fas fa-times"></i> Batal
              </button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
  <script>
    let rooms = [];
    let availablePatients = [];
    const addRoomModal = new bootstrap.Modal(document.getElementById('addRoomModal'));
    const editRoomModal = new bootstrap.Modal(document.getElementById('editRoomModal'));
    const assignPatientModal = new bootstrap.Modal(document.getElementById('assignPatientModal'));

    async function loadRooms() {
      try {
        console.log('üì° Loading rooms...');
        const response = await fetch('/api/rooms');
        
        if (!response.ok) {
          throw new Error(`HTTP ${response.status}: ${response.statusText}`);
        }
        
        const result = await response.json();
        console.log('‚úì Rooms:', result);
        
        if (result.success) {
          rooms = result.rooms || [];
          renderRooms();
        } else {
          showAlert('danger', result.error || 'Gagal memuat data');
        }
      } catch (error) {
        console.error('‚ùå Error:', error);
        showAlert('danger', 'Error: ' + error.message);
        document.getElementById('rooms-container').innerHTML = `
          <div class="col-12">
            <div class="alert alert-danger">
              <i class="fas fa-exclamation-triangle"></i>
              <strong>Error:</strong> ${error.message}
            </div>
          </div>
        `;
      }
    }

    async function loadAvailablePatients() {
      try {
        const response = await fetch('/api/rooms/available-patients');
        if (!response.ok) throw new Error(`HTTP ${response.status}`);
        
        const result = await response.json();
        availablePatients = result.patients || [];
        populatePatientDropdowns();
      } catch (error) {
        console.error('Error loading patients:', error);
        availablePatients = [];
      }
    }

    function populatePatientDropdowns() {
      const addSelect = document.getElementById('add-emr-no');
      const assignSelect = document.getElementById('assign-emr-no');
      
      const options = availablePatients.map(p => 
        `<option value="${p.emr_no}">${p.nama} (EMR: ${p.emr_no})</option>`
      ).join('');
      
      addSelect.innerHTML = '<option value="">-- Ruangan Kosong --</option>' + options;
      assignSelect.innerHTML = '<option value="">-- Pilih Pasien --</option>' + options;
    }

    function renderRooms() {
      const container = document.getElementById('rooms-container');
      
      if (!rooms || rooms.length === 0) {
        container.innerHTML = `
          <div class="col-12">
            <div class="empty-state">
              <i class="fas fa-door-open"></i>
              <h3>Belum Ada Ruangan</h3>
              <p>Tambahkan ruangan baru untuk memulai</p>
              <button class="btn btn-add-room" onclick="showAddRoomModal()">
                <i class="fas fa-plus"></i> Tambah Ruangan Pertama
              </button>
            </div>
          </div>
        `;
        return;
      }

      const html = rooms.map(room => {
        const isOccupied = room.emr_no !== null;
        const statusClass = isOccupied ? 'occupied' : 'empty';
        const statusText = isOccupied ? 'TERISI' : 'KOSONG';
        const icon = isOccupied ? 'fa-door-closed' : 'fa-door-open';
        
        return `
          <div class="col-md-6 col-lg-4">
            <div class="room-card">
              <div class="room-header ${statusClass}">
                <span class="room-status-badge">${statusText}</span>
                <h3 class="room-id"><i class="fas ${icon}"></i> ${room.room_id}</h3>
              </div>
              <div class="room-body">
                <div class="info-row">
                  <i class="fas fa-microchip"></i>
                  <span class="info-label">Device:</span>
                  <span class="info-value">${room.device_id}</span>
                </div>
                ${isOccupied ? `
                  <div class="info-row">
                    <i class="fas fa-user"></i>
                    <span class="info-label">Pasien:</span>
                    <span class="info-value">${room.nama_pasien}</span>
                  </div>
                  <div class="info-row">
                    <i class="fas fa-id-card"></i>
                    <span class="info-label">EMR:</span>
                    <span class="info-value">${room.emr_no}</span>
                  </div>
                  <div class="info-row">
                    <i class="fas fa-hospital"></i>
                    <span class="info-label">Poli:</span>
                    <span class="info-value">${room.poli}</span>
                  </div>
                ` : `
                  <div class="alert alert-secondary mb-0">
                    <i class="fas fa-info-circle"></i> Ruangan kosong
                  </div>
                `}
                <div class="room-actions">
                  ${isOccupied ? `
                    <button class="btn-room-action btn-remove" onclick="removePatient('${room.room_id}')">
                      <i class="fas fa-door-open"></i> Keluarkan
                    </button>
                  ` : `
                    <button class="btn-room-action btn-assign" onclick="showAssignModal('${room.room_id}')">
                      <i class="fas fa-user-plus"></i> Masukkan
                    </button>
                  `}
                  <button class="btn-room-action btn-edit" onclick="showEditModal('${room.room_id}', '${room.device_id}')">
                    <i class="fas fa-edit"></i> Edit
                  </button>
                  <button class="btn-room-action btn-delete" onclick="deleteRoom('${room.room_id}')">
                    <i class="fas fa-trash"></i> Hapus
                  </button>
                </div>
              </div>
            </div>
          </div>
        `;
      }).join('');
      
      container.innerHTML = html;
    }

    function showAddRoomModal() {
      document.getElementById('form-add-room').reset();
      loadAvailablePatients();
      addRoomModal.show();
    }

    function showEditModal(roomId, deviceId) {
      document.getElementById('edit-old-room-id').value = roomId;
      document.getElementById('edit-room-id').value = roomId;
      document.getElementById('edit-device-id').value = deviceId;
      editRoomModal.show();
    }

    function showAssignModal(roomId) {
      document.getElementById('assign-room-id').value = roomId;
      document.getElementById('assign-room-display').textContent = roomId;
      loadAvailablePatients();
      assignPatientModal.show();
    }

    document.getElementById('form-add-room').addEventListener('submit', async (e) => {
      e.preventDefault();
      try {
        const response = await fetch('/api/rooms/add', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({
            room_id: document.getElementById('add-room-id').value.trim(),
            device_id: document.getElementById('add-device-id').value.trim(),
            emr_no: document.getElementById('add-emr-no').value || null
          })
        });
        const result = await response.json();
        if (result.success) {
          showAlert('success', 'Ruangan berhasil ditambahkan');
          addRoomModal.hide();
          loadRooms();
          loadAvailablePatients();
        } else {
          showAlert('danger', result.error);
        }
      } catch (error) {
        showAlert('danger', 'Error: ' + error.message);
      }
    });

    document.getElementById('form-edit-room').addEventListener('submit', async (e) => {
      e.preventDefault();
      try {
        const response = await fetch(`/api/rooms/${document.getElementById('edit-old-room-id').value}`, {
          method: 'PUT',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({
            new_room_id: document.getElementById('edit-room-id').value.trim(),
            device_id: document.getElementById('edit-device-id').value.trim()
          })
        });
        const result = await response.json();
        if (result.success) {
          showAlert('success', 'Ruangan berhasil diupdate');
          editRoomModal.hide();
          loadRooms();
        } else {
          showAlert('danger', result.error);
        }
      } catch (error) {
        showAlert('danger', 'Error: ' + error.message);
      }
    });

    document.getElementById('form-assign-patient').addEventListener('submit', async (e) => {
      e.preventDefault();
      try {
        const response = await fetch('/api/rooms/assign', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({
            room_id: document.getElementById('assign-room-id').value,
            emr_no: parseInt(document.getElementById('assign-emr-no').value)
          })
        });
        const result = await response.json();
        if (result.success) {
          showAlert('success', 'Pasien berhasil dimasukkan');
          assignPatientModal.hide();
          loadRooms();
          loadAvailablePatients();
        } else {
          showAlert('danger', result.error);
        }
      } catch (error) {
        showAlert('danger', 'Error: ' + error.message);
      }
    });

    async function removePatient(roomId) {
      if (!confirm(`Yakin keluarkan pasien dari ${roomId}?`)) return;
      try {
        const response = await fetch('/api/rooms/remove-patient', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({ room_id: roomId })
        });
        const result = await response.json();
        if (result.success) {
          showAlert('success', 'Pasien berhasil dikeluarkan');
          loadRooms();
          loadAvailablePatients();
        } else {
          showAlert('danger', result.error);
        }
      } catch (error) {
        showAlert('danger', 'Error: ' + error.message);
      }
    }

    async function deleteRoom(roomId) {
      if (!confirm(`Yakin hapus ruangan ${roomId}?\n\nRuangan harus kosong.`)) return;
      try {
        const response = await fetch('/api/rooms/delete', {
          method: 'DELETE',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({ room_id: roomId })
        });
        const result = await response.json();
        if (result.success) {
          showAlert('success', 'Ruangan berhasil dihapus');
          loadRooms();
        } else {
          showAlert('danger', result.error);
        }
      } catch (error) {
        showAlert('danger', 'Error: ' + error.message);
      }
    }

    function filterRooms() {
      const searchTerm = document.getElementById('search-rooms').value.toLowerCase();
      const roomCards = document.querySelectorAll('.room-card');
      
      roomCards.forEach(card => {
        const roomId = card.querySelector('.room-id').textContent.toLowerCase();
        const deviceId = card.querySelector('.info-value').textContent.toLowerCase();
        const patientName = card.querySelector('.info-value:nth-child(2)')?.textContent.toLowerCase() || '';
        const emrNo = card.querySelector('.info-value:nth-child(3)')?.textContent.toLowerCase() || '';
        
        const matches = roomId.includes(searchTerm) || 
                       deviceId.includes(searchTerm) || 
                       patientName.includes(searchTerm) || 
                       emrNo.includes(searchTerm);
        
        card.style.display = matches ? 'block' : 'none';
      });
    }

    // Auto filter on input change
    document.getElementById('search-rooms').addEventListener('input', filterRooms);

    document.addEventListener('DOMContentLoaded', () => {
      console.log('‚úì Page loaded');
      loadRooms();
      loadAvailablePatients();
    });
  </script>
</body>
</html>