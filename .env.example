# .env.example
# Copy file ini ke .env dan isi dengan values yang sesuai

# Database Configuration
MYSQL_ROOT_PASSWORD=your_root_password
MYSQL_DATABASE=darsinurse
MYSQL_USER=your_db_user
MYSQL_PASSWORD=your_db_password

# Database Connection
DB_HOST=darsinurse-db
DB_PORT=3306
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=darsinurse

# Application Secrets (GANTI dengan random string!)
SESSION_SECRET_RAWATJALAN=change-this-to-random-string
SESSION_SECRET_MONITORING=change-this-to-random-string

# MQTT Configuration (for Vitals API)
MQTT_URL=mqtt://103.106.72.181:1883
MQTT_USERNAME=MEDLOC
MQTT_PASSWORD=MEDLOC
FALLBACK_EMR=UNASSIGNED

# Metabase
METABASE_URL=https://metabase.darsinurse.hint-lab.id
METABASE_SECRET=your_metabase_secret

# Allowed Origins (pisahkan dengan koma)
ALLOWED_ORIGINS=https://darsinurse.hint-lab.id,https://gateway.darsinurse.hint-lab.id

# Timezone
TZ=Asia/Jakarta